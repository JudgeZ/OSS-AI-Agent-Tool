{
  "name": "oss-ai-agent-tool-cli",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "aidt": "dist/index.js"
  },
  "scripts": {
    "prebuild": "npm install --prefix ../../services/orchestrator --include=dev --no-package-lock",
    "build": "npm run build --prefix ../../services/orchestrator && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "npm run build && node --test tests/plan.test.js"
  },
  "dependencies": {
    "@oss/orchestrator": "file:../../services/orchestrator"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
